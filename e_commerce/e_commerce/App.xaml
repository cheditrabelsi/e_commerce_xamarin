<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="e_commerce.App">
    <Application.Resources>
        <ResourceDictionary>
            
            <OnPlatform x:Key="FontAwesomeSolid" x:TypeArguments="x:String">
                <On Platform="iOS" Value="Font Awesome 5 Free-Solid-900.otf#Regular" />
                <On Platform="Android" Value="Font Awesome 5 Free-Solid-900.otf#Regular" />
            </OnPlatform>

           
            <OnPlatform x:Key="PlusIcon" x:TypeArguments="x:String">
                <On Platform="iOS" Value="&#xf067;" />
                <On Platform="Android" Value="&#xf067;" />
            </OnPlatform>

            <OnPlatform x:Key="MinusIcon" x:TypeArguments="x:String">
                <On Platform="iOS" Value="&#xf068;" />
                <On Platform="Android" Value="&#xf068;" />
            </OnPlatform>

        </ResourceDictionary>
    </Application.Resources>
</Application>